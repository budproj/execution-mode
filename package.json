{
  "name": "execution-mode",
  "version": "0.0.1",
  "description": "Web interface for Bud's execution mode",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "run-s intl:refresh server:watch",
    "server:watch": "nodemon",
    "server:launch": "ts-node --project tsconfig.server.json ./src/index.ts",
    "build": "run-s clean compile copy",
    "compile": "run-s compile:intl compile:typescript",
    "compile:intl": "run-p intl:compile:ptBR intl:compile:enUS",
    "compile:typescript": "tsc --project tsconfig.server.json",
    "clean": "rimraf -rf dist",
    "copy": "cp README.md LICENSE package.json dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "intl:generate:id": "eslint */**/messages.ts --fix",
    "intl:refresh": "run-s intl:generate:id intl:extract",
    "intl:extract": "formatjs extract './src/**/messages.ts' --out-file lang/pt-BR.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile:ptBR": "formatjs compile lang/pt-BR.json --ast --out-file compiled-lang/pt-BR.json",
    "intl:compile:enUS": "formatjs compile lang/en-US.json --ast --out-file compiled-lang/en-US.json"
  },
  "author": "delucca <delucca@pm.me>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budproj/execution-mode.git"
  },
  "bugs": {
    "url": "https://github.com/budproj/execution-mode/issues"
  },
  "engines": {
    "yarn": ">=2.3.3",
    "node": ">=15.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^2.13.5",
    "@tsconfig/recommended": "^1.0.1",
    "@types/accepts": "^1",
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/glob": "^7.1.3",
    "@types/koa": "^2.11.6",
    "@types/koa__router": "^8.0.3",
    "@types/lodash": "^4",
    "@types/node": "^14.14.3",
    "@types/prettier": "^2",
    "@types/react": "^16.9.53",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-editorconfig": "^2.1.3",
    "eslint-plugin-formatjs": "^2.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@koa/router": "^9.4.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "accepts": "^1.3.7",
    "chalk": "^4.1.0",
    "crypto": "^1.0.1",
    "glob": "^7.1.6",
    "koa": "^2.13.0",
    "lodash": "^4.17.20",
    "next": "^10.0.0",
    "react": "^16.14.0",
    "react-dom": "^17.0.1",
    "react-intl": "^5.8.8",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  }
}
