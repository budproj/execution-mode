{
  "name": "execution-mode",
  "version": "1.0.0",
  "description": "Web interface for Bud's execution mode",
  "main": "src/server/index.js",
  "scripts": {
    "prepare": "[ \"$NODE_ENV\" != production ] && husky install",
    "start": "NODE_ENV=production node ./dist/index.js",
    "build": "npm-run-all -s clean build:app compile",
    "build:app": "next build",
    "start:dev": "npm-run-all -s intl:refresh server:watch",
    "server:watch": "nodemon",
    "server:launch": "ts-node --project tsconfig.server.json ./src/server/index.ts",
    "compile": "run-s compile:intl compile:server",
    "compile:intl": "npm-run-all -p intl:compile:ptBR intl:compile:enUS",
    "compile:server": "tsc --project ./tsconfig.server.json",
    "clean": "rimraf -rf dist .next",
    "copy": "cp README.md LICENSE package.json dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . --ext ts,tsx",
    "lint:staged": "lint-staged",
    "lint:fix": "npm run lint -- --fix",
    "lint:timing": "TIMING=1 eslint . --ext ts,tsx",
    "lint:messages": "eslint **/messages.ts --fix",
    "format:and:lint": "npm-run-all -s format lint:fix",
    "intl:generate:id": "echo '\u001b[36mâž¤ \u001b[90mGenerating react-intl message ids...\u001b[39m' && npm run lint:messages",
    "intl:refresh": "npm-run-all -s intl:generate:id intl:extract",
    "intl:extract": "formatjs extract './src/**/*.ts(x)?' --out-file lang/pt-BR.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile:ptBR": "formatjs compile lang/pt-BR.json --ast --out-file compiled-lang/pt-BR.json",
    "intl:compile:enUS": "formatjs compile lang/en-US.json --ast --out-file compiled-lang/en-US.json",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "deploy": "run-s git:tag:canary build waypoint:build waypoint:deploy",
    "release": "run-s git:tag:remove-canary git:tag:stable waypoint:up",
    "git:tag:canary": "git tag canary -f && git push origin canary -f",
    "git:tag:remove-canary": "git tag -d canary",
    "git:tag:stable": "git tag stable -f && git push origin stable -f",
    "waypoint:build": "waypoint build",
    "waypoint:deploy": "waypoint deploy",
    "waypoint:up": "waypoint up"
  },
  "author": "delucca <delucca@pm.me>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budproj/execution-mode.git"
  },
  "bugs": {
    "url": "https://github.com/budproj/execution-mode/issues"
  },
  "engines": {
    "npm": ">=6.14.11",
    "node": ">=15.2.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.7",
    "@budproj/eslint-config-base-ts": "^1.0.1",
    "@budproj/eslint-config-intl": "^1.0.0",
    "@budproj/eslint-config-prettier-ts": "^1.0.0",
    "@budproj/eslint-config-react": "^1.0.0",
    "@budproj/prettier-config": "^1.0.0",
    "@formatjs/cli": "^2.13.5",
    "@miragejs/graphql": "^0.1.9",
    "@tsconfig/recommended": "^1.0.1",
    "@types/accepts": "^1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/faker": "^5",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/koa": "^2.11.6",
    "@types/koa__router": "^8.0.3",
    "@types/lodash": "^4",
    "@types/node": "^14.14.3",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2",
    "@types/react": "^16.9.53",
    "@types/react-beautiful-dnd": "^13",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/recoil": "^0",
    "@types/rimraf": "^3",
    "@types/sinon": "^9.0.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
